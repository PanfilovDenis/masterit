= simple_form_for @competition, url: url, html: { class: 'default_class' } do |f|
  = f.input :state_event, as: :state_event
  = f.input :name
  = f.input :year
  = f.association :tours
  .form-actions
    = f.button :submit, class: 'btn btn-primary'
    = link_to t('.cancel'), admin_competitions_path, class: 'btn'
